{
  "extends": [
    "wesbos",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "no-console": 0,
    "jsx-props-no-spreading": 0,
    "no-prototype-builtins": 0,
    "no-plusplus": 0,
    "max-classes-per-file": ["error",10], // for older bundled files
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4,
      }
    ]
  }
}